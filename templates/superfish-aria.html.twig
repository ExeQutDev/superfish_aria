{#
/**
 * @file
 * Default theme implementation of the Superfish menus.
 *
 * Available variables:
 * - id: Unique menu identifier.
 * - menu_classes: Menu classes.
 * - menu_items: Menu tree.
 *
 * @see template_preprocess_superfish()
 *
 * @ingroup themeable
 */
#}
{{ attach_library('superfish_aria/superfisharia') }}
<nav class="primary-nav" aria-label="main menu">
<ul id="{{ id }}" class="{{ menu_classes }}" role="menubar" aria-label="Navigation Menu">
  {{ menu_items }}
</ul>
</nav>